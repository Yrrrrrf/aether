{
  "name": "@yrrrrrf/aether",
  "version": "0.1.0",
  "exports": {
    ".": "./src/runtime/mod.ts",
    "./generator": "./src/oracle/mod.ts"
  },
  "tasks": {
    "dev": "deno test --watch",
    "generate": "deno run --allow-net --allow-read --allow-write --allow-env src/oracle/cli.ts",
    "test": "deno test --allow-net --allow-read --allow-write --allow-env tests/"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@1",
    "@std/dotenv": "jsr:@std/dotenv@0.225.2",
    "postgres": "https://deno.land/x/postgres@v0.19.3/mod.ts"
  },
  "fmt": {
    "singleQuote": false,
    "semiColons": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  }
}
